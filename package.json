{
  "name": "testcafe-testing",
  "version": "1.0.0",
  "description": "Automated testing with TestCafe",
  "main": "index.js",
  "scripts": {
    "test:chrome": "testcafe chrome ./tests -s takeOnFails=true --disable-page-caching",
    "test:chrome:headless": "testcafe chrome:headlesss ./tests -s takeOnFails=true",
    "test:chrome:mobile": "testcafe chrome:emulation:device=iphone X ./tests -s takeOnFails=true",
    "test:safari": "testcafe safari ./tests -s takeOnFails=true",
    "test:firefox": "testcafe firefox ./tests -s takeOnFails=true"
  },
  "keywords": [
    "TestCafe",
    "Testing",
    "Automation"
  ],
  "author": "Tevan",
  "license": "ISC",
  "dependencies": {
    "prettier": "^2.3.2",
    "testcafe": "^1.15.1"
  }
}
